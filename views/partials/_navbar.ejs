<div class="header clearfix">
    <nav>
        <ul class="nav nav-pills pull-right">
            <% if (siteData.userLoggedIn) { %>
                <li role="presentation"><p class="navbar-text"><%= siteData.userLoggedInEmail %></p></li>
                <li role="presentation"><button type="button" class="btn logout" name="logout" >Logout</button></li>
            <% } else { %>
                <li role="presentation"><a href="/login">Login</a></li>
                <li role="presentation"><a href="/register">Register</a></li>
            <% } %>
        </ul>
        <h3 class="text-muted">TinyApp URL Shortener</h3>
    </nav>
</div>